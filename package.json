{
  "name": "node-lib-starter",
  "version": "0.0.0",
  "description": "My awesome typescript library",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --fix --ext .vue,.js,.ts,.jsx,.tsx",
    "build": "tsup && esno scripts/postbuild.ts",
    "test": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@sxzz/eslint-config-prettier": "^1.6.1",
    "@sxzz/eslint-config-ts": "^1.6.2",
    "@types/node": "*",
    "eslint": "^8.9.0",
    "eslint-define-config": "^1.2.5",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "prettier": "^2.5.1",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "vitest": "^0.4.2"
  }
}
